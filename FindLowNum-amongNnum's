/*
Program to print Lowest among n numbers
Assingment-1 : Day-17 : Author-Tanishq Deerghasi 
*/


#include<stdio.h>
#include<stdlib.h>
int main()
{
	int n,*p,i,small;
	printf("Enter array size");
	scanf("%d",&n);
	
	p=(int*) malloc(n*sizeof(int));
	
	printf("Enter %d array elements",n);
	
	for(i=0;i<n;i++)
		scanf("%d",&p[i]);
	
	small=p[0];
	
	for(i=0;i<n;i++)
	{
		if(p[i]<small)
			small=p[i];
	}
	
	printf("Lowest is %d",small);
	
	return 0;
}
