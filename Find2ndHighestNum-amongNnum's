/*
Program to print second highest among n numbers
Assingment-2 : Day-17 : Author-Tanishq Deerghasi 
*/

#include<stdio.h>
#include<stdlib.h>
int main()
{
	int n,*a,i,j,t;
	printf("Enter array size");
	scanf("%d",&n);
	
	a=(int*) malloc(n*sizeof(int));
	
	printf("Enter %d array elements",n);
	
	 for(i=0;i<n;i++)
		scanf("%d",&a[i]);
      
      for(i=0;i<n;i++)
      {
            for(j=i+1;j<n;j++)
            {
                  if((a[i])<(a[j]))
                  {
                        t=a[i];
                        a[i]=a[j];
                        a[j]=t;
                  }
            }
      }
	
	printf("Second Highest is %d",a[1]);
	return 0;
}
