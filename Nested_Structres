/*
Title:Write a program to demonstrate nested structures
Author: Tanishq Deerghasi
Date: 24-03-2023
*/

#include<stdio.h>
struct student
{
	char name[30];
	int sno;
	struct date
	{
		int dd;
		int mm;
		int yy;
	}dob;
};
void main()
{
	struct student s;
	printf("Enter name");
	gets(s.name);
	printf("Enter Sno");
	scanf("%d",&s.sno);
	printf("Enter date of birht(dd/mm/yy) format:");
	scanf("%d %d %d",&s.dob.dd,&s.dob.mm,&s.dob.yy);
	
	printf("\nHello %s",s.name);
	printf("\nYour ID no: ADCSKLM0%d",s.sno);
	printf("\nand you born on: %d/%d/%d",s.dob.dd,s.dob.mm,s.dob.yy);
}
