/*
Program to print 10 numbers between 1 and 2
Assingment-1 : Day-15 : Author-Tanishq Deerghasi 
*/

#include<stdio.h>
int main()
{
	int a,b,t,j;
	float i;
	printf("Enter two numbers");
	scanf("%d %d",&a,&b);
	
	if(a<b)
	{
		printf("The 10 numbers between %d and %d are\n",a,b);
		for(i=a,j=1;i<=b;i=i+0.10,j++)
		{
			printf("%d\t%.2f\n",j,i);
			if(i==b)
				break;
		}
	}
	else if(a==b)
		printf("Operation Not Possible\n");
	else
	{
		t=a;
		a=b;
		b=t;
		
		printf("The 10 numbers between %d and %d are\n",a,b);
		for(i=a,j=1;i<=b;i=i+0.10,j++)
		{
			printf("%d\t%.2f\n",j,i);
			if(i==b)
				break;
		}
		
	}
	return 0;
}
