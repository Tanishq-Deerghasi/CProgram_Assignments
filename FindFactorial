/*
Title:Program to find factorial
Author: Tanishq Deerghasi
Date:17-03-2023
*/

#include<stdio.h>
int fact(int x)
{
	if(x==1 || x==0)
		return 1;
	else
		return x*fact(x-1);
}

int main()
{
	int f,n;
	printf("Enter a number");
	scanf("%d",&n);
	f = fact(n);
	printf("Factorial of %d is %d",n,f);
	return 0;
}
