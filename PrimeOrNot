/*
Program to check prime or not
Assingment-3 : Day-17 : Author-Tanishq Deerghasi 
*/

#include<stdio.h>
int main()
{
	int n,i,flag=0;
	printf("Enter number");
	scanf("%d",&n);
		
	for(i=2;i<=n-1;i++)
	{
		if(n%i==0)
		{
			flag=1;
			break;
		}
	}
	
	if(flag==0)
		printf("PRIME");
	else
		printf("NOT PRIME");
	return 0;
}
