/*
Write a program to print factors count of the given number.
Assingment-2 : Day-22 : Author-Tanishq Deerghasi 
*/

#include<stdio.h>
int main()
{
	int n,i,count=0;
	
	printf("Enter a num");
	scanf("%d",&n);
	for(i=1;i<=n;i++)
	{
		if(n%i==0)
		{
			//printf("%d\t",i);
				count++;
		}		
	}
	
	printf("%d",count);
	return 0;
}
